{
  "manifest_version": 2,

  "name": "Access Log",
  "description": "This extension will log the Accessbility Issues.",
  "author": "Venkatraman Ramamoorthy",
  "version": "0.1",

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "tabs", "http://*/", "https://*/"
 ],

  "browser_action": {
    "default_icon": "alog.png"
  },

  "icons" : {
    "48" : "alog-48.png",
    "128" : "alog-128.png"
  }
}