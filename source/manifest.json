{
  "manifest_version": 2,
  "name": "Access Log",
  "version": "0.1",

  "default_locale": "en",
  "description": "This extension will log the Accessbility Issues.",
  "author": "Venkatraman Ramamoorthy",

  "icons" : {
    "16": "alog.png",
    "48" : "alog-48.png",
    "128" : "alog-128.png"
  }

  "browser_action": {
    "default_icon": {
      "16": "alog.png",
      "48" : "alog-48.png",
      "128" : "alog-128.png"
    },
    "default_title": "Access Log"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "tabs",
    "http://*/",
    "https://*/"
  ]
}
